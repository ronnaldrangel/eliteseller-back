{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description_wsp": {
      "type": "text"
    },
    "description_complete": {
      "type": "string"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "chatbot": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::chatbot.chatbot",
      "inversedBy": "products"
    },
    "available": {
      "type": "boolean"
    },
    "stock": {
      "type": "integer"
    }
  }
}
