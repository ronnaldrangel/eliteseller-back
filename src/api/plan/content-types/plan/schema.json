{
  "kind": "collectionType",
  "collectionName": "plans",
  "info": {
    "singularName": "plan",
    "pluralName": "plans",
    "displayName": "Plan"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "plan_id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "billing_interval": {
      "type": "integer"
    },
    "billing_period": {
      "type": "string"
    },
    "features": {
      "type": "json"
    },
    "regular_price": {
      "type": "integer"
    },
    "subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subscription.subscription",
      "mappedBy": "plan"
    },
    "trial_price": {
      "type": "decimal"
    },
    "has_trial": {
      "type": "boolean",
      "default": false
    },
    "tokens_add": {
      "type": "biginteger"
    },
    "message_add": {
      "type": "biginteger"
    }
  }
}
