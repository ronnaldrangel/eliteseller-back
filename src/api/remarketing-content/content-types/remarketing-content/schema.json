{
  "kind": "collectionType",
  "collectionName": "remarketing_contents",
  "info": {
    "singularName": "remarketing-content",
    "pluralName": "remarketing-contents",
    "displayName": "Remarketing_Content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "text"
    },
    "remarketing": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::remarketing.remarketing",
      "inversedBy": "remarketing_contents"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "time_to_send": {
      "type": "integer"
    }
  }
}
